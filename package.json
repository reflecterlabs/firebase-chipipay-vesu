{
  "name": "vesuhooks",
  "version": "1.0.0",
  "description": "Vesu Hooks with ChipiPay Integration",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
